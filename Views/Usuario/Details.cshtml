@{
    var usuario = ViewData["Usuario"] as DesenvolvedorNET.Models.Usuario;
}
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <title>@ViewData["Title"]</title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
</head>
<body>
    <h1>@ViewData["Title"]</h1>
    <table>
        <tr>
            <td>Id</td>
            <td>@usuario.Id</td>
        </tr>
        <tr>
            <td>Nome</td>
            <td>@usuario.Nome</td>
    </table>
</body>
</html>