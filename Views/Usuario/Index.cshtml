@model List<DesenvolvedorNET.Models.Usuario>
@{
    Layout = "_Layout.cshtml";
    var usuarios = ViewData["Usuarios"] as List<DesenvolvedorNET.Models.Usuario>;
}
<h1>@ViewData["Title"]</h1>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Ações</th>
        </tr>
    </thead>
    @foreach (var usuario in Model)
    {
        <tr>
            <td>@usuario.Id</td>
            <td>@usuario.Nome</td>
            <td>
                <a href="@Url.Action("details","usuario", new{id = usuario.Id})">Detalhes</a> |
                @Html.ActionLink("Editar","edit","usuario", new{id = usuario.Id})  |
                <a href="/usuario/delete/@usuario.Id">Deletar</a> 
            </td>
        </tr>
    }
</table>