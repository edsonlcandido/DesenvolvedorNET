@{
    var usuarios = ViewData["Usuarios"] as List<DesenvolvedorNET.Models.Usuario>;
}
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <title>@ViewData["Title"]</title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
</head>
<body>
    <h1>@ViewData["Title"]</h1>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Ações</th>
            </tr>
        </thead>
        @foreach (var usuario in usuarios)
        {
            <tr>
                <td>@usuario.Id</td>
                <td>@usuario.Nome</td>
                <td>
                    <a href="/usuario/details/@usuario.Id">Detalhes</a> |
                    <a href="/usuario/edit/@usuario.Id">Editar</a> |
                    <a>Excluir</a>
                </td>
            </tr>
        }
    </table>
</body>
</html>